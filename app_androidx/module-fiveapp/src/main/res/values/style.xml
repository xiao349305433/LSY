<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--基站首页网格文字样式-->
    <style name="m_five_station_grid_tv">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#9f9f9f</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:padding">2dp</item>
    </style>
    <!--基站首页线性点击列表文字样式-->
    <style name="m_five_station_line_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/l_five_Q</item>
    </style>
    <!--基站首页线性点击列表布局样式-->
    <style name="m_five_station_lv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>
    <!--基站首页基站定位等条目的图标 类目名 和更多的父布局样式-->
    <style name="m_five_station_rrl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10dp</item>
    </style>
    <!--基站首页更点的图片样式-->
    <style name="m_five_station_more_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/more</item>
    </style>
</resources>
