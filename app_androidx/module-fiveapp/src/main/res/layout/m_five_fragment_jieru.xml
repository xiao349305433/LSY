<?xml version="1.0" encoding="utf-8"?>

<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.flyco.roundview.RoundLinearLayout
        android:id="@+id/linear_benDi"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:elevation="2dp"
        android:orientation="vertical"
        app:rv_backgroundColor="@color/base_white"
        app:rv_cornerRadius="2dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:background="#E2E3E6"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:text="民用接入"
                android:textColor="@color/l_five_Q" />
        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />

        <TextView
            android:id="@+id/round_yuan_cheng_biaohao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:drawableEnd="@drawable/more"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:text="远传表号接入(LSY-M01)"
            android:textSize="15sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />
        <LinearLayout
            android:layout_width="match_parent"
            android:background="#E2E3E6"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:text="民用接入"
                android:textColor="@color/l_five_Q" />
        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />

        <TextView
            android:id="@+id/round_wulianwangduan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:drawableEnd="@drawable/more"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:text="物联网端(LSY-M02)"
            android:textSize="15sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />

        <LinearLayout
            android:layout_width="match_parent"
            android:background="#E2E3E6"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:text="公用接入"
                android:textColor="@color/l_five_Q" />
        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />

        <TextView
            android:id="@+id/round_liuliangji"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:drawableEnd="@drawable/more"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:text="4号通用模组初始化(LSY-M04)"
            android:textSize="15sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/base_global_neutral_gray_c6c9d4" />

        <TextView
            android:id="@+id/round_liuliangji_upload"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:drawableEnd="@drawable/more"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:text="4号通用模组定位(LSY-M04)"
            android:textSize="15sp"
            android:textStyle="bold" />

    </com.flyco.roundview.RoundLinearLayout>
</androidx.core.widget.NestedScrollView>