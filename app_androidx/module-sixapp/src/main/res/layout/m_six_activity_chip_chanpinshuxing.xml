    <LinearLayout
        android:id="@+id/linear_environmental2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@drawable/drawer_bluecontent_layout_tv_shape"
        android:orientation="vertical"
        android:paddingLeft="9dp">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_tiaoshi_send"
                android:layout_width="64dp"
                android:layout_height="40dp"
                android:background="@drawable/drawer_bluecontent_bluetv_shape"
                android:gravity="center"
                android:text="启动"
                android:textColor="@color/white"></TextView>

            <ImageView
                android:id="@+id/img_tiaoshi"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginLeft="10dp"
                ></ImageView>


        </LinearLayout>
        <TextView
            android:id="@+id/tv_environmental2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:lineSpacingExtra="10dp"
            android:textSize="16sp"></TextView>


